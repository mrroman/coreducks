{
    "env": {
        "node": true,
        "mocha": true,
        "browser": true,
        "es6": true
    },
    "ecmaFeatures": {
        "blockBindings": true,
        "destructuring": true,
        "forOf": true,
        "object-shorthand": [2, "always"],
        "modules": true
    },
    "globals": {
        "$": false,
        "interact": false
    },
    "plugins": [],
    "rules": {
        "block-scoped-var": 2,
        "brace-style": [2, "1tbs", { "allowSingleLine": true }],
        "camelcase": 2,
        "comma-style": [2],
        "curly": [2, "all"],
        "dot-notation": [2, {"allowKeywords": true}],
        "eol-last": 2,
        "eqeqeq": [2, "allow-null"],
        "strict": [2, "never"],
        "guard-for-in": 2,
        "new-cap": 2,
        "new-parens": 2,
        "no-bitwise": 2,
        "no-caller": 2,
        "no-cond-assign": [2, "except-parens"],
        "no-debugger": 2,
        "no-empty": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-parens": 0,
        "no-extra-semi": 2,
        "no-irregular-whitespace": 2,
        "no-iterator": 2,
        "no-loop-func": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-multiple-empty-lines": 2,
        "no-multi-str": 2,
        "no-nested-ternary": 2,
        "no-new": 2,
        "no-plusplus": 2,
        "no-proto": 2,
        "no-script-url": 2,
        "no-sequences": 2,
        "no-shadow": 2,
        "no-trailing-spaces": 2,
        "no-undef": 2,
        "no-unneeded-ternary": 2,
        "no-unused-vars": 2,
        "no-with": 2,
        "linebreak-style": 2,
        "quotes": [2, "single"],
        "semi": [2, "always"],
        "space-after-keywords": 2,
        "valid-typeof": 2,
        "wrap-iife": [2, "inside"]
    }
}
